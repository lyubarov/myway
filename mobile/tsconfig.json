{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@services/*": ["services/*"],
      "@navigation/*": ["src/navigation/*"],
      "@appTypes/*": ["src/appTypes/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
